<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open Cart Regression Suite" verbose="4" thread-count="2" parallel="tests">
<listeners>
<listener class-name="RetryLogic.AnnotationTransformer"></listener>
</listeners>
  <test  name="Test">
  <parameter name = "url" value ="https://www.google.com"/>
  <parameter name = "browser" value ="edge"/>
    <classes>
      <class name="com.qa.Testcases.GoogleTest"/>
      </classes>
  </test> <!-- Test -->
  
  <test  name="OpenCart Test">
  <parameter name = "url" value ="https://naveenautomationlabs.com/opencart/index.php?route=account/login"/>
  <parameter name = "browser" value ="chrome"/>
    <classes>
      <class name="com.qa.Testcases.OpenCartTest"/>
      </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
